 <value>222</value>
 </property>
 
 
 <property name="studentName">
 <value>Ram</value>
 </property>
 
 
 <property name="studentAddress" value="Mumbai"/>
 
 
 </bean>
 
  <bean class="com.springcore.Student" name="student3" p:studentId="55" p:studentName="Geeta" p:studentAddress="Punjab"/>
 
 </beans>